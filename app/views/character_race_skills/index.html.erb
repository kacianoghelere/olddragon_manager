<p id="notice"><%= notice %></p>

<h1>Listing Character Race Skills</h1>

<table>
  <thead>
    <tr>
      <th>Character race</th>
      <th>Skill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @character_race_skills.each do |character_race_skill| %>
      <tr>
        <td><%= character_race_skill.character_race %></td>
        <td><%= character_race_skill.skill %></td>
        <td><%= link_to 'Show', character_race_skill %></td>
        <td><%= link_to 'Edit', edit_character_race_skill_path(character_race_skill) %></td>
        <td><%= link_to 'Destroy', character_race_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Character race skill', new_character_race_skill_path %>
